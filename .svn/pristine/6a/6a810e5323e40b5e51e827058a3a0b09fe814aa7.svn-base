<%@ page language="java" pageEncoding="UTF-8"%>

<form id="" name="" class="form-horizontal" autocomplete="off"
	method="post" data-bv-message="This value is not valid"
	data-bv-feedbackicons-valid="glyphicon glyphicon-ok"
	data-bv-feedbackicons-invalid="glyphicon glyphicon-remove"
	data-bv-feedbackicons-validating="glyphicon glyphicon-refresh">
	
	<div class="modal fade" id="info_list">
		<div class="modal-dialog modal-lg" style="width:680px">
			<div class="modal-content">
			
				<div class="modal-header" style="height: 40px">
					<button type="button" class="btn btn-link close" data-dismiss="modal">
						<span aria-hidden="true">×</span><span class="sr-only">关闭</span>
					</button>
					<h4 class="modal-title" style="line-height:15px;"><i class="icon icon-home">&nbsp;人员详细</i></h4>
				</div>
				
				<div class="modal-body">
					<table class="table table-bordered">
						<tr>
							<td class="td-label"><span class="tr-span">姓名：</span></td>
							<td ><span class="td-span" id="STAFFNAME_list"></span></td>
							<td class="td-label"><span class="tr-span">职称：</span></td>
							<td ><span class="td-span" id="POSITION_list"></span></td>
						</tr>
						<tr>
							<td class="td-label"><span class="tr-span">联系电话：</span></td>
							<td ><span class="td-span" id="LINKPHONE_list"></span></td>
							<td class="td-label"><span class="tr-span">固定电话：</span></td>
							<td ><span  class="td-span" id="FIXTELE_list"></span></td>
						</tr>
						<tr>
							<td class="td-label"><span class="tr-span">邮箱：</span></td>
							<td colspan="3"><span class="td-span" id="EMAIL_list"></span></td>
						</tr>
						<tr>
							<td class="td-label"><span class="tr-span">性别：</span></td>
							<td colspan="3"><span class="td-span" id="SEX_list"></span>
							</td>
						</tr>
						<tr>
							<td class="td-label"><span class="tr-span">地址：</span></td>
							<td colspan="3"><span class="td-span"  id="ADDRESS_list"></span></td>
						</tr>
						<tr>
                 	 		<td class="td-label"><span class="tr-span">备注：</span></td>
                	 		<td colspan="3"><span class="td-span" id="REMARK_list"></span></td>
                  		</tr> 
					</table>
				</div>
				
				<div class="modal-footer foter" >
					<button type="button" class="btn btn-large bottom-btn" data-dismiss="modal">
						<i class="icon icon-times "></i> 取消
					</button>
				</div>
			
			</div>
		</div>
	</div>
	
</form>