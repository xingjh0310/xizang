/*
尺寸开始
*/
/*面包屑*/
h1, h2, h3 {
    margin-top: 15px;
    margin-bottom: 10px;
}
.breadcrumb{
    padding-top: 16px;
    padding-left: 7px;
	height: 20px;
	margin-bottom: -2px;
}
/*主界面*/
#maincontent {
    padding-left: 7px;
    padding-right: 7px;
}
/*工具栏*/
hr {
    margin-bottom: 15px;
    margin-top: 20px;
}
.btn{
	font-size:14px;
}
/*table表头*/
.fixed-table-header {
    font-size: 14px;
}

/* 对话框 */
.modal-header {
    padding-left: 10px;
}
.modal-body {
    padding-left: 10px;
    padding-right: 10px;
}
.btn{
	border-radius: 3px;
}

/*
尺寸结束
*/




/*颜色开始*/
.alert-danger {
    background-color: #f8ead1;
    color: #e75033;
}
/*颜色结束*/