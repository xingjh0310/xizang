<%@ page language="java" pageEncoding="UTF-8"%>
<style>
#photo_quertion{
	display:flex;
	flex-wrap:wrap;
}
.img_css{
	width:80px;
	height:80px;
	margin:8px;
}
</style>

<form id="" name="" class="form-horizontal" autocomplete="off"
	method="post" data-bv-message="This value is not valid"
	data-bv-feedbackicons-valid="glyphicon glyphicon-ok"
	data-bv-feedbackicons-invalid="glyphicon glyphicon-remove"
	data-bv-feedbackicons-validating="glyphicon glyphicon-refresh">
	<div class="modal fade" id="info_list">
		<div class="modal-dialog modal-lg" style="width:680px">
			<div class="modal-content">
				<div class="modal-header" style="height: 40px">
					<button type="button" class="btn btn-link close modal_header_top" data-dismiss="modal">
						<span aria-hidden="true"><i class="icon icon-times"></i></span><span class="sr-only">关闭</span>
					</button>
					<h4 class="modal-title modal_header_top"><i class="icon icon-home"></i>&nbsp;物资问题详细信息</h4>
				</div>
				<div class="modal-body">
					<table class="table table-bordered info_table_css">
						<tr>
							<td style="width:100px;text-align:right;font-weight:bold;background: #f1f1f1">
                            	<label style="margin:0">标&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;题：</label>
							</td>
							<td colspan="3">
                	 			<lable  style="margin-left: 15px;" id="TITLE_quertion"></lable>
                	 		</td>
						</tr>
						<tr>
							<td style="width:100px;text-align:right;font-weight:bold;background: #f1f1f1">
                            	<label style="margin:0">物资名称：</label>
							</td>
							<td style="width:200px;">
								<lable style="margin-left: 15px;" id="MATERIELNAME_quertion"></lable>
							</td>
							<td style="width:100px;text-align:right;font-weight:bold;background: #f1f1f1">
                            	<label style="margin:0">问题类型：</label>
							</td>
							<td style="width:200px;">
								<lable style="margin-left: 15px;" id="DICTNAME_quertion"></lable>
							</td>
						</tr>
						<tr>
							<td style="width:100px;text-align:right;font-weight:bold;background: #f1f1f1">
                            	<label style="margin:0">上报单位：</label>
							</td>
							<td style="width:200px;">
								<lable style="margin-left: 15px;" id="DEPTNAME_quertion"></lable>
							</td>
							<td style="width:100px;text-align:right;font-weight:bold;background: #f1f1f1">
                            	<label style="margin:0">上&nbsp;&nbsp;报&nbsp;人：</label>
							</td>
							<td style="width:200px;">
								<lable style="margin-left: 15px;" id="STAFFNAME_quertion"></lable>
							</td>
						</tr>
						<tr>
							<td style="width:100px;text-align:right;font-weight:bold;background: #f1f1f1">
                            	<label style="margin:0">上报时间：</label>
							</td>
							<td style="width:200px;">
								<lable style="margin-left: 15px;" id="REPORTTIME_quertion"></lable>
							</td>
							<td style="width:100px;text-align:right;font-weight:bold;background: #f1f1f1">
                            	<label style="margin:0">处理期限：</label>
							</td>
							<td style="width:200px;">
								<lable style="margin-left: 15px;" id="PROCESSLIMIT_quertion"></lable>
							</td>
						</tr>
						<tr>
							<td style="width:100px;text-align:right;font-weight:bold;background: #f1f1f1">
                            	<label style="margin:0">问题描述：</label>
							</td>
							<td colspan="3" style="padding:0">
								<lable style="margin-left: 15px;" id="PROBLEMDESC_quertion"></lable>
							</td>
						</tr>
						<tr>
							<td style="width:100px;text-align:right;font-weight:bold;background: #f1f1f1">
                            	<label style="margin:0">问题照片：</label>
							</td>
							<td colspan="3" style="padding:0">
								<div id="photo_quertion"></div>
							</td>
						</tr>
						<tr>
							<td style="width:100px;text-align:right;font-weight:bold;background: #f1f1f1">
                            	<label style="margin:0">备&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;注：</label>
							</td>
							<td colspan="3" style="padding:0">
								<lable style="margin-left: 15px;" id="REMARK_quertion"></lable>
							</td>
						</tr>
					</table>
				</div>
				
				<div class="modal-footer" style="height: 40px;">
					<button type="button" class="btn" data-dismiss="modal" style="margin-top: -14px;margin-right:-5px">
						<i class="icon icon-times"></i> 取消
					</button>
				</div>
				
			</div>
		</div>		
	</div>
</form>